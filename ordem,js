const usuarios = [
    {nome: "Ana", idade: 32},
    {nome: "mila", idade: 28},
    {nome: "felipe", idade: 40}
];

//encontrando um objeto

const ana = usuarios.find(usuario => usuario.nome ===  "Ana");
console.log(ana);

//filtrando objetos

const usuarioAcimaDe30 = usuarios.filter(usuario => usuario.idade > 30);
console.log(usuarioAcimaDe30);

//Ordenando objetos por idade

const usuariosOrdemPorIdade = usuarios.sort((a,b) => a.idade - b.idade);
console.log(usuariosOrdemPorIdade)
